platform :ios, '11.0'

plugin 'cocoapods-keys', {
  :project => "GithubBrowser",
  :keys => [
    "GithubClientId",
    "GithubClientSecret"
  ]}

target 'GithubBrowser' do
  inhibit_all_warnings!
  use_frameworks!

  pod 'SnapKit', '~> 5.0'
  pod 'Swinject'
  pod 'SwinjectAutoregistration'
  pod 'Alamofire'
end

